# Example .env for Auth Service

# Server
HTTP_PORT=8001
GRPC_PORT=9001

# Database
DATABASE_URL=postgres://tunduck_user:tunduck_password@localhost:5432/tunduck_auth

# Redis
REDIS_ADDR=localhost:6379

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# JWT
JWT_SECRET=your-super-secret-key-change-this-in-production-please
JWT_EXPIRES=3600

# Environment
ENVIRONMENT=development
